{
  "env": {
    "node": true
  },
  "rules": {
    "brace-style": [2, "1tbs", { "allowSingleLine": false }],
    "camelcase": [0],
    "comma-spacing": 0,
    "dot-notation": 0,
    "eol-last": [0],
    "hft-camelcase": [2],
    "key-spacing": [0],
    "no-comma-dangle": [0],
    "no-multi-spaces": [0],
    //"no-redeclare": [0],
    "no-shadow": [0],
    "no-undef": [0],
    "no-undef-init": [0],
    "one-variable-per-var/one-variable-per-var": [2],
    "quotes": [0, "single"],
    "require-trailing-comma/require-trailing-comma": [2],
  }
}
