{
  "env": {
    "node": true
  },
  "rules": {
    "brace-style": [2, "1tbs", { "allowSingleLine": false }],
    "camelcase": [0],
    "google-camelcase/google-camelcase": [2],
    "key-spacing": [0],
    "no-comma-dangle": [0],
    "no-multi-spaces": [0],
    "one-variable-per-var/one-variable-per-var": [2],
    "quotes": [2, "single"],
    "require-trailing-comma/require-trailing-comma": [2],
  }
}
